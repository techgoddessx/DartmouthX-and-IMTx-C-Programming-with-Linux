#include <stdio.h>
#include <stdlib.h>

struct digit {
	int number;
	struct digit * next;
};
struct student *creatememory(int digit);
int main(void){
	struct student * pointer;
	int thedigit = 1;
	pointer = creatememory(thedigit);
	printf("%p : %d",pointer,pointer->number);
	free(pointer);
	return 0;
}
struct student *creatememory(int digit){
	struct student * ptr;
	ptr = (struct student *) malloc(sizeof(struct student));
	ptr->number = digit;
	ptr->next = NULL;
	return ptr;
}
